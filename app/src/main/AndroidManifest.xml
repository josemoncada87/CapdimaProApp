<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.icesi.gleo.capdimaproapp">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.SignupActivity" />
        <activity android:name=".activities.MainMenuActivity" />
        <activity android:name=".activities.InitialHomeSettingsActivity" />
        <activity android:name=".activities.NeighborsDefinitionActivity" />
        <activity android:name=".activities.CreateSpacesActivity" />
        <activity android:name=".activities.FecadeSelectionActivity" />
        <activity android:name=".activities.OpenHouseActivity"></activity>
    </application>

</manifest>